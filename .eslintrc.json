{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eqeqeq": "error",
        "no-console": "warn",
        "quotes": [
            "error",
            "double",
            {
                "avoidEscape": true
            }
        ],
        "comma-dangle": "off"
    }
}
